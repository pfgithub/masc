.text            # 
subu $sp, $sp 8  # $sp = $sp - 8;
move $t0 $sp     # var stack_value: [*]u32 = $sp;
li $t1 5         # save stack_value[0] = 5;
sw $t1 ($t0)     # 
li $t1 10        # save stack_value[1] = 10;
sw $t1 4($t0)    # 
lw $t1 4($t0)    # var value: u32 = stack_value[1];
la $t1 4($t0)    # var pointer: [*]u32 = &stack_value[1];
                 # 
li $t1 5         # var index: u32 = 5;
mulo $t0, $t1 4  # var unused: u32 = stack_value[index];
add $t0, $t0 $t0 # 
lw $t0 ($t0)     # 
                 # 
move $t0 $sp     # var stack_u8: [*]u8 = $sp;
add $t0, $t1 $t0 # var unused2: u8 = stack_u8[index];
lb $t0 ($t0)     # 